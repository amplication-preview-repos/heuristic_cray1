datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model BlogPost {
  author        String?
  content       String?
  createdAt     DateTime  @default(now())
  id            String    @id @default(cuid())
  publishedDate DateTime?
  title         String?
  updatedAt     DateTime  @updatedAt
}

model EventLog {
  createdAt DateTime  @default(now())
  eventType String?
  id        String    @id @default(cuid())
  payload   Json?
  timestamp DateTime?
  updatedAt DateTime  @updatedAt
}
